package pt.ulisboa.tecnico.sirs.medicalrecords.personal;

class Person {
    String name;
    DateTime birthday;
    long identification;
}

class Doctor extends Person;
class Patient extends Person;

class Nurse extends Person;


relation DomainRootHasPersonApplication {
	.pt.ist.fenixframework.DomainRoot playsRole root {
		multiplicity 1..1;
	}
	Doctor playsRole Doctor {
		multiplicity 0..*;
	}

	Patient playsRole patient{
		multiplicity 0..*;
	}
}

